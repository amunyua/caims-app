<?xml version='1.0' encoding='utf-8'?>
<widget id="com.openpath.countyassembly" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>County Assembly</name>
    <description>
        App for sharing county assembly documents
    </description>
    <author email="alexmunyua10@gmail.com" href="http://cordova.io">
        Alex Munyua
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
